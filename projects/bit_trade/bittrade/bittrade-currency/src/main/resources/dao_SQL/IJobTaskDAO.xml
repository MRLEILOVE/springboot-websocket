<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bittrade.api.dao.IJobTaskDAO">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.bittrade.pojo.model.JobTask">
		<id javaType="int" property="id" jdbcType="INTEGER" column="id" />
		<result javaType="String" property="content" jdbcType="VARCHAR" column="content" />
		<result javaType="long" property="sendTime" jdbcType="BIGINT" column="send_time" />
		<result javaType="int" property="status" jdbcType="INTEGER" column="status" />
	</resultMap>

	<!-- 通用查询结果列 -->
	<sql id="Base_Column_List">
		`content` AS content, 
		`send_time` AS sendTime, 
		`status` AS status
	</sql>

</mapper>