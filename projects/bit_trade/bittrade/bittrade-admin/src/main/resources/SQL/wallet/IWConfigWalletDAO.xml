<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bittrade.admin.dao.wallet.IWConfigWalletDAO">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.bittrade.pojo.model.WConfigWallet">
		<id javaType="int" property="id" jdbcType="INTEGER" column="id" />
		<result javaType="String" property="coinType" jdbcType="VARCHAR" column="coin_type" />
		<result javaType="String" property="walletType" jdbcType="VARCHAR" column="wallet_type" />
		<result javaType="String" property="address" jdbcType="VARCHAR" column="address" />
		<result javaType="String" property="keystore" jdbcType="VARCHAR" column="keystore" />
		<result javaType="String" property="valid" jdbcType="CHAR" column="valid" />
		<result javaType="String" property="remark" jdbcType="VARCHAR" column="remark" />
		<result javaType="java.time.LocalDateTime" property="createTime" jdbcType="TIMESTAMP" column="create_time" />
		<result javaType="java.time.LocalDateTime" property="updateTime" jdbcType="TIMESTAMP" column="update_time" />
	</resultMap>

	<!-- 通用查询结果列 -->
	<sql id="Base_Column_List">
		`id` AS id, 
		`coin_type` AS coinType, 
		`wallet_type` AS walletType, 
		`address` AS address, 
		`keystore` AS keystore, 
		`valid` AS valid, 
		`remark` AS remark, 
		`create_time` AS createTime, 
		`update_time` AS updateTime
	</sql>

</mapper>