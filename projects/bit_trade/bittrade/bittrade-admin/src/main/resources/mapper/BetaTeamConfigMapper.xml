<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jdcloud.provider.mapper.BetaTeamConfigMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jdcloud.provider.pojo.BetaTeamConfig">
        <id column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="remark" property="remark" />
        <result column="name" property="name" />
        <result column="earnings_ratio" property="earningsRatio" />
        <result column="peers_earnings_ratio" property="peersEarningsRatio" />
        <result column="direct_ask" property="directAsk" />
        <result column="direct_team_ask" property="directTeamAsk" />
        <result column="team_ask" property="teamAsk" />
        <result column="earnings_ask" property="earningsAsk" />
        <result column="direct_partner_ask" property="directPartnerAsk" />
        <result column="partner_ask" property="partnerAsk" />
        <result column="pack_number" property="packNumber" />
        <result column="pack_money" property="packMoney" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_time AS createTime, update_time AS updateTime, remark,
        name, earnings_ratio AS earningsRatio, peers_earnings_ratio AS peersEarningsRatio,
        direct_ask AS directAsk, direct_team_ask AS directTeamAsk, team_ask AS teamAsk,
        earnings_ask AS earningsAsk, direct_partner_ask AS directPartnerAsk, partner_ask AS partnerAsk,
        pack_number AS packNumber, pack_money AS packMoney
    </sql>

    <delete id="deleteConfigByIds" parameterType="Integer">
        delete from g_beta_team_config where id in
        <foreach collection="array" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>
