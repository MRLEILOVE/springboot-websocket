<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bittrade.currency.dao.ITPaymentDAO">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.bittrade.pojo.model.TPayment">
		<id javaType="int" property="id" jdbcType="INTEGER" column="id" />
		<result javaType="Integer" property="id" jdbcType="INTEGER" column="id" />
		<result javaType="String" property="payNameCn" jdbcType="VARCHAR" column="pay_name_cn" />
		<result javaType="String" property="payNameEn" jdbcType="VARCHAR" column="pay_name_en" />
		<result javaType="java.time.LocalDateTime" property="createTime" jdbcType="TIMESTAMP" column="create_time" />
		<result javaType="java.time.LocalDateTime" property="updateTime" jdbcType="TIMESTAMP" column="update_time" />
	</resultMap>

	<!-- 通用查询结果列 -->
	<sql id="Base_Column_List">
		`id` AS id, 
		`pay_name_cn` AS payNameCn, 
		`pay_name_en` AS payNameEn, 
		`create_time` AS createTime, 
		`update_time` AS updateTime
	</sql>

</mapper>