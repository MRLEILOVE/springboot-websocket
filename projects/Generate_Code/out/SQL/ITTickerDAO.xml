<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bittrade.currency.dao.ITTickerDAO">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.bittrade.pojo.model.TTicker">
		<id javaType="int" property="id" jdbcType="INTEGER" column="id" />
		<result javaType="String" property="symbol" jdbcType="VARCHAR" column="symbol" />
		<result javaType="String" property="last" jdbcType="VARCHAR" column="last" />
		<result javaType="java.util.Date" property="createTime" jdbcType="TIMESTAMP" column="create_time" />
	</resultMap>

	<!-- 通用查询结果列 -->
	<sql id="Base_Column_List">
		`symbol` AS symbol, 
		`last` AS last, 
		`create_time` AS createTime
	</sql>

</mapper>