		<result javaType="java.util.Date" property="updateTime" jdbcType="TIMESTAMP" column="update_time" />
	</resultMap>

	<!-- 通用查询结果列 -->
	<sql id="Base_Column_List">
		`id` AS id, 
		`platform` AS platform, 
		`user_id` AS userId, 
		`coin_type` AS coinType, 
		`address` AS address, 
		`private_key` AS privateKey, 
		`flag` AS flag, 
