<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bittrade.currency.dao.ISysUserOnlineDAO">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.bittrade.pojo.model.SysUserOnline">
		<id javaType="int" property="id" jdbcType="INTEGER" column="id" />
		<result javaType="String" property="loginName" jdbcType="VARCHAR" column="login_name" />
		<result javaType="String" property="deptName" jdbcType="VARCHAR" column="dept_name" />
		<result javaType="String" property="ipaddr" jdbcType="VARCHAR" column="ipaddr" />
		<result javaType="String" property="loginLocation" jdbcType="VARCHAR" column="login_location" />
		<result javaType="String" property="browser" jdbcType="VARCHAR" column="browser" />
		<result javaType="String" property="os" jdbcType="VARCHAR" column="os" />
		<result javaType="String" property="status" jdbcType="VARCHAR" column="status" />
		<result javaType="java.time.LocalDateTime" property="startTimestamp" jdbcType="TIMESTAMP" column="start_timestamp" />
		<result javaType="java.time.LocalDateTime" property="lastAccessTime" jdbcType="TIMESTAMP" column="last_access_time" />
		<result javaType="Integer" property="expireTime" jdbcType="INTEGER" column="expire_time" />
	</resultMap>

	<!-- 通用查询结果列 -->
	<sql id="Base_Column_List">
		`sessionId` AS sessionId, 
		`login_name` AS loginName, 
		`dept_name` AS deptName, 
		`ipaddr` AS ipaddr, 
		`login_location` AS loginLocation, 
		`browser` AS browser, 
		`os` AS os, 
		`status` AS status, 
		`start_timestamp` AS startTimestamp, 
		`last_access_time` AS lastAccessTime, 
		`expire_time` AS expireTime
	</sql>

</mapper>