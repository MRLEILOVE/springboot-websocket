		<result javaType="Integer" property="operatorType" jdbcType="INTEGER" column="operator_type" />
		<result javaType="String" property="operName" jdbcType="VARCHAR" column="oper_name" />
		<result javaType="String" property="deptName" jdbcType="VARCHAR" column="dept_name" />
		<result javaType="String" property="operUrl" jdbcType="VARCHAR" column="oper_url" />
