		`dept_name` AS deptName, 
		`oper_url` AS operUrl, 
		`oper_ip` AS operIp, 
		`oper_location` AS operLocation, 
		`oper_param` AS operParam, 
		`status` AS status, 
		`error_msg` AS errorMsg, 
		`oper_time` AS operTime
	</sql>

</mapper>