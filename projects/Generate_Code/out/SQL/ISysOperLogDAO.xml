	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.bittrade.pojo.model.SysOperLog">
		<id javaType="int" property="id" jdbcType="INTEGER" column="id" />
		<result javaType="String" property="title" jdbcType="VARCHAR" column="title" />
